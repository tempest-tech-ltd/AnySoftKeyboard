<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
                android:shape="rectangle">
            <padding android:top="@dimen/key_right_inset"
                     android:right="@dimen/key_right_inset"
                     android:bottom="@dimen/key_bottom_inset"
                     android:left="@dimen/key_right_inset"/>
            <solid android:color="@color/keyboard_bg_normal_color"/>
            <stroke android:width="2px" android:color="@color/tv_text_color"/>
            <corners android:radius="@dimen/key_corner_radius"/>
        </shape>
    </item>
</layer-list>
